{
  "name": "my-template",
  "version": "1.0.0",
  "description": "Starter to create a template for DeckDeckGo presentations",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/my-template/my-template.esm.js",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@deckdeckgo/slide-utils": "^2.6.1"
  },
  "devDependencies": {
    "@stencil/core": "^2.3.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/sass": "^1.4.1",
    "@types/node": "^14.14.22",
    "autoprefixer": "^10.2.1",
    "husky": "^4.3.8",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deckgo/template-kit.git"
  },
  "author": {
    "name": "David Dal Busco",
    "url": "https://daviddalbusco.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deckgo/template-kit"
  },
  "homepage": "https://deckdeckgo.com",
  "keywords": [
    "stencil",
    "stenciljs",
    "web components",
    "pwa",
    "progressive web app",
    "presentation",
    "slides",
    "slideshow",
    "talk",
    "templates"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
